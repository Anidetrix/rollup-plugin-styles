{
  "name": "rollup-plugin-styles",
  "version": "3.14.1",
  "description": "Universal Rollup plugin for styles: PostCSS, Sass, Less, Stylus and more",
  "keywords": [
    "rollup",
    "rollup-plugin",
    "css",
    "css-modules",
    "postcss",
    "sass",
    "scss",
    "less",
    "stylus"
  ],
  "homepage": "https://github.com/Anidetrix/rollup-plugin-styles#readme",
  "bugs": {
    "url": "https://github.com/Anidetrix/rollup-plugin-styles/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anidetrix/rollup-plugin-styles.git"
  },
  "license": "MIT",
  "author": "Anton Kudryavtsev <boblobl4@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build": "rollup -c --environment NODE_ENV:production",
    "postbuild": "shx cp -r src/shims dist",
    "commit": "commit",
    "predocs": "shx rm -rf docs",
    "docs": "typedoc",
    "postdocs": "shx cp .ghpages.yml docs/_config.yml",
    "docs:publish": "npm run docs && gh-pages -d docs",
    "prelint": "prettier -l --write .",
    "lint": "eslint --report-unused-disable-directives --ext .ts,.js --fix .",
    "prepublishOnly": "npm run build",
    "release": "semantic-release",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.1.0",
    "@types/cssnano": "^4.0.0",
    "cosmiconfig": "^7.0.0",
    "cssnano": "^4.1.10",
    "fs-extra": "^9.1.0",
    "icss-utils": "^5.1.0",
    "mime-types": "^2.1.28",
    "p-queue": "^6.6.2",
    "postcss": "^8.2.4",
    "postcss-modules-extract-imports": "^3.0.0",
    "postcss-modules-local-by-default": "^4.0.0",
    "postcss-modules-scope": "^3.0.0",
    "postcss-modules-values": "^4.0.0",
    "postcss-value-parser": "^4.1.0",
    "query-string": "^6.13.8",
    "resolve": "^1.19.0",
    "source-map": "^0.7.3",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@commitlint/prompt-cli": "^11.0.0",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/fs-extra": "^9.0.6",
    "@types/jest": "^26.0.20",
    "@types/mime-types": "^2.1.0",
    "@types/node": "*",
    "@types/resolve": "^1.19.0",
    "@types/uglifycss": "^0.0.6",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "autoprefixer": "^10.2.3",
    "builtin-modules": "^3.2.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^27.0.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "less": "^4.1.0",
    "lint-staged": "^10.5.3",
    "minireset.css": "^0.0.7",
    "node-sass": "^5.0.0",
    "postcss-custom-properties": "^11.0.0",
    "prettier": "^2.2.1",
    "rollup": "^2.38.0",
    "rollup-plugin-dts": "^2.0.1",
    "rollup-plugin-lit-css": "^2.1.0",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.5",
    "semantic-release": "^17.3.7",
    "shx": "^0.3.3",
    "stylus": "^0.54.8",
    "sugarss": "^3.0.3",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.20.19",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "rollup": "^2.2.0"
  },
  "engines": {
    "node": ">=10"
  }
}
