{
  "name": "rollup-plugin-styles",
  "version": "3.0.4",
  "description": "Universal Rollup plugin for styles: PostCSS, Sass, Less, Stylus and more",
  "keywords": [
    "rollup",
    "rollup-plugin",
    "css",
    "css-modules",
    "postcss",
    "sass",
    "scss",
    "less",
    "stylus"
  ],
  "homepage": "https://github.com/Anidetrix/rollup-plugin-styles#readme",
  "bugs": {
    "url": "https://github.com/Anidetrix/rollup-plugin-styles/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anidetrix/rollup-plugin-styles.git"
  },
  "license": "MIT",
  "author": "Anton Kudryavtsev <boblobl4@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c --environment NODE_ENV:development",
    "build:prod": "rollup -c --environment NODE_ENV:production",
    "commit": "commit",
    "docs": "shx rm -rf docs && typedoc && gh-pages -d docs",
    "format": "prettier -l --write .",
    "lint": "npm run format && eslint --ext .ts,.js .",
    "lint:fix": "npm run format && eslint --ext .ts,.js --fix .",
    "prepublishOnly": "shx rm -rf dist && npm run build:prod && shx cp -r src/typings dist",
    "release": "semantic-release",
    "test": "npm run lint:fix && jest"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "@rollup/pluginutils": "^3.0.10",
    "concat-with-sourcemaps": "^1.1.0",
    "cssnano": "^4.1.10",
    "fs-extra": "^9.0.0",
    "icss-utils": "^4.1.1",
    "mime-types": "^2.1.27",
    "p-queue": "^6.4.0",
    "postcss": "^7.0.29",
    "postcss-load-config": "^2.1.0",
    "postcss-modules-extract-imports": "^2.0.0",
    "postcss-modules-local-by-default": "^3.0.2",
    "postcss-modules-scope": "^2.2.0",
    "postcss-modules-values": "^3.0.0",
    "postcss-value-parser": "^4.1.0",
    "resolve": "^1.17.0",
    "source-map": "^0.7.3",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@commitlint/prompt-cli": "^8.3.5",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^4.1.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/cssnano": "^4.0.0",
    "@types/fs-extra": "^8.1.0",
    "@types/icss-utils": "^4.1.0",
    "@types/jest": "^25.2.1",
    "@types/mime-types": "^2.1.0",
    "@types/node": "*",
    "@types/postcss-load-config": "^2.0.1",
    "@types/resolve": "^1.17.0",
    "@types/uglifycss": "^0.0.5",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "autoprefixer": "^9.7.6",
    "babel-jest": "^26.0.1",
    "babel-plugin-transform-node-env-inline": "^0.4.3",
    "builtin-modules": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^19.0.1",
    "fibers": "^5.0.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "less": "^3.11.1",
    "lint-staged": "^10.2.2",
    "node-sass": "^4.14.1",
    "postcss-custom-properties": "^9.1.1",
    "prettier": "^2.0.5",
    "rollup": "^2.7.6",
    "rollup-plugin-dts": "^1.4.0",
    "rollup-plugin-lit-css": "^2.0.3",
    "rollup-plugin-node-externals": "^2.1.6",
    "rollup-plugin-terser": "^5.3.0",
    "sass": "^1.26.5",
    "semantic-release": "^17.0.7",
    "shx": "^0.3.2",
    "stylus": "^0.54.7",
    "sugarss": "^2.0.0",
    "ts-jest": "^25.4.0",
    "typedoc": "^0.17.6",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "rollup": "^2.2.0"
  },
  "engines": {
    "node": ">=10"
  }
}
