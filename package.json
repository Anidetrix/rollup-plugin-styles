{
  "name": "rollup-plugin-styles",
  "version": "0.0.0-dev",
  "description": "Universal Rollup plugin for styles: PostCSS, Sass, Less, Stylus and more",
  "main": "dist/rollup-plugin-styles.js",
  "module": "dist/rollup-plugin-styles.es.js",
  "types": "dist/rollup-plugin-styles.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "docgen": "typedoc",
    "format": "prettier -l --write \"**/*.{ts,js,json,yml,md}\"",
    "lint": "npm run format && eslint .",
    "lint:fix": "npm run format && eslint --fix .",
    "test": "npm run lint:fix && jest",
    "test:cov": "npm run lint:fix && jest --coverage",
    "commit": "commit",
    "prepublishOnly": "shx rm -rf dist && npm run build && shx cp -r runtime dist && shx rm -rf \"dist/**/.eslintrc*\" \"dist/*.es.d.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anidetrix/rollup-plugin-styles.git"
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "css",
    "css-modules",
    "postcss",
    "sass",
    "scss",
    "less",
    "stylus"
  ],
  "author": "Anton Kudryavtsev <boblobl4@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Anidetrix/rollup-plugin-styles/issues"
  },
  "engines": {
    "node": ">=10"
  },
  "homepage": "https://github.com/Anidetrix/rollup-plugin-styles#readme",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@commitlint/prompt-cli": "^8.3.5",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@types/cssnano": "^4.0.0",
    "@types/fs-extra": "^8.1.0",
    "@types/icss-utils": "^4.1.0",
    "@types/jest": "^25.1.4",
    "@types/less": "^3.0.1",
    "@types/node": "*",
    "@types/postcss-load-config": "^2.0.1",
    "@types/resolve": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "@wessberg/rollup-plugin-ts": "^1.2.21",
    "autoprefixer": "^9.7.4",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-unicorn": "^17.2.0",
    "fibers": "^4.0.2",
    "fs-extra": "^9.0.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "less": "^3.11.1",
    "lint-staged": "^10.0.8",
    "node-sass": "^4.13.1",
    "postcss-custom-properties": "^9.1.1",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.19.1",
    "rollup": "^2.1.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "sass": "^1.26.3",
    "shx": "^0.3.2",
    "stylus": "^0.54.7",
    "sugarss": "^2.0.0",
    "ts-jest": "^25.2.1",
    "tslib": "^1.11.1",
    "typedoc": "^0.17.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@rollup/pluginutils": "^3.0.8",
    "concat-with-sourcemaps": "^1.1.0",
    "cssnano": "^4.1.10",
    "hash.js": "^1.1.7",
    "icss-utils": "^4.1.1",
    "p-queue": "^6.3.0",
    "postcss": "^7.0.27",
    "postcss-load-config": "^2.1.0",
    "postcss-modules-extract-imports": "^2.0.0",
    "postcss-modules-local-by-default": "^3.0.2",
    "postcss-modules-scope": "^2.2.0",
    "postcss-modules-values": "^3.0.0",
    "resolve": "^1.15.1"
  },
  "peerDependencies": {
    "rollup": "^1.20.0||^2.0.0"
  },
  "release": {
    "branches": [
      "master"
    ]
  }
}
