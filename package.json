{
  "name": "rollup-plugin-styles",
  "version": "3.8.1",
  "description": "Universal Rollup plugin for styles: PostCSS, Sass, Less, Stylus and more",
  "keywords": [
    "rollup",
    "rollup-plugin",
    "css",
    "css-modules",
    "postcss",
    "sass",
    "scss",
    "less",
    "stylus"
  ],
  "homepage": "https://github.com/Anidetrix/rollup-plugin-styles#readme",
  "bugs": {
    "url": "https://github.com/Anidetrix/rollup-plugin-styles/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anidetrix/rollup-plugin-styles.git"
  },
  "license": "MIT",
  "author": "Anton Kudryavtsev <boblobl4@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build": "rollup -c --environment NODE_ENV:production",
    "postbuild": "shx cp -r src/shims dist",
    "commit": "commit",
    "predocs": "npm run build && shx rm -rf docs",
    "docs": "typedoc dist",
    "postdocs": "shx cp .ghpages.yml docs/_config.yml",
    "docs:publish": "npm run docs && gh-pages -d docs",
    "prelint": "prettier -l --write .",
    "lint": "eslint --ext .ts,.js --fix .",
    "prepublishOnly": "npm run build",
    "release": "semantic-release",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "dependencies": {
    "@rollup/pluginutils": "^3.1.0",
    "cssnano": "^4.1.10",
    "fs-extra": "^9.0.1",
    "icss-utils": "^4.1.1",
    "mime-types": "^2.1.27",
    "p-queue": "^6.4.0",
    "postcss": "^7.0.32",
    "postcss-load-config": "^2.1.0",
    "postcss-modules-extract-imports": "^2.0.0",
    "postcss-modules-local-by-default": "^3.0.2",
    "postcss-modules-scope": "^2.2.0",
    "postcss-modules-values": "^3.0.0",
    "postcss-value-parser": "^4.1.0",
    "resolve": "^1.17.0",
    "source-map": "^0.7.3",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@commitlint/prompt-cli": "^8.3.5",
    "@rollup/plugin-babel": "^5.0.3",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@rollup/plugin-typescript": "^4.1.2",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/cssnano": "^4.0.0",
    "@types/fs-extra": "^9.0.1",
    "@types/icss-utils": "^4.1.0",
    "@types/jest": "^25.2.3",
    "@types/mime-types": "^2.1.0",
    "@types/node": "*",
    "@types/postcss-load-config": "^2.0.1",
    "@types/resolve": "^1.17.1",
    "@types/uglifycss": "^0.0.6",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "autoprefixer": "^9.8.0",
    "babel-plugin-transform-node-env-inline": "^0.4.3",
    "builtin-modules": "^3.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^20.1.0",
    "gh-pages": "^3.0.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-circus": "^26.0.1",
    "less": "^3.11.3",
    "lint-staged": "^10.2.9",
    "minireset.css": "^0.0.6",
    "node-sass": "^4.14.1",
    "postcss-custom-properties": "^9.1.1",
    "prettier": "^2.0.5",
    "rollup": "^2.15.0",
    "rollup-plugin-dts": "^1.4.7",
    "rollup-plugin-lit-css": "^2.0.4",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-terser": "^6.1.0",
    "sass": "^1.26.8",
    "semantic-release": "^17.0.8",
    "shx": "^0.3.2",
    "stylus": "^0.54.7",
    "sugarss": "^2.0.0",
    "ts-jest": "^26.1.0",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "rollup": "^2.2.0"
  },
  "engines": {
    "node": ">=10"
  }
}
